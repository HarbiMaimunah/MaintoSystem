<h1> Respond To Ticket </h1>
@section Scripts2{
    <script src="~/js/MaintenanceManager/bundle.min.js"></script>
    <body>
        <label>Ticket ID:</label>
        <input type="number" id="TicketId" name="TicketId" placeholder="Id" />

        <label>Status</label>
        <select name="Status" id="Status">
            <option value="Pending">Pending</option>
            <option value="Rejected">Rejected</option>
        </select>

        <label>Reason</label>
        <input type="text" id="reason" name="reason" placeholder="Reason" />

        <button type="button" id="btnPost" name="btnPost" data-name="btnPost" onclick="RespondToTicket()"> Respond To Ticket </button>
    </body>



}